{
    "name": "net.egmsystems/yii2-asset-choices",
    "description": "yii2 asset to https://github.com/Choices-js/Choices",
    "type": "yii2-extension",
    "keywords": ["php","yii2","extension","asset","js","choices.js"],
    "license": "MIT",
    "homepage": "https://github.com/egmsystems/yii2-asset-choices",
    "support": {
        "issues": "https://github.com/egmsystems/yii2-asset-choices/issues",
        "source": "https://github.com/egmsystems/yii2-asset-choices/tree/main/src"
    },
    "authors": [
        {
            "name": "Edgar Gonzalez",
            "email": "yii2-asset-choices@EGMSystems.net",
            "homepage": "https://egmsystems.net?yii2-asset-choices"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "egmyii\\choicejs\\": "src"
        }
    },
    "repositories":[
        {
            "type": "git",
            "url": "https://github.com/egmsystems/yii2-asset-choices"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "pre-install-cmd": ["egmyii\\choicejs\\ComposerScript::preInstallCmd"],
        "pre-update-cmd": ["egmyii\\choicejs\\ComposerScript::preInstallCmd"],
        "post-install-cmd": ["egmyii\\choicejs\\ComposerScript::postInstallCmd"],
        "post-update-cmd": ["egmyii\\choicejs\\ComposerScript::postInstallCmd"]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.7-dev"
        },
        "egmyii\\ComposerScript::postInstallCmd": {
            "setPermission": [
                {
                    "z:/runtimePath": "0777"
                }
            ]
        }
    }
}
